{
  "name": "vsc-landingpage",
  "displayName": "vsc-landingpage",
  "description": "Welcome page with project bookmark manager.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ "onStartupFinished" ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsc-landingpage.showWelcomePage",
        "title": "Show Landing Page"
      }
    ],
    "configuration": 
    {
      "title": "Landingpage",
      "properties": {
        "vsc-landingpage.autoshowOnStartup": 
        {
          "type": "boolean",
          "default": true,
          "description": "Open Landingpage automatically in empty windows."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "lucide-static": "^0.471.0",
    "svg-inline-loader": "^0.8.2"
  }
}
